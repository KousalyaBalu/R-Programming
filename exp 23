# Given vectors for the columns
col1 <- c(1, 2, 3)
col2 <- c(4, 5, 6)
col3 <- c(7, 8, 9)
col4 <- c(10, 11, 12)

# Combine vectors into a matrix with 4 columns
matrix_data <- cbind(col1, col2, col3, col4)

# Create a 3x4x2 array
my_array <- array(data = matrix_data, dim = c(3, 4, 2))

# Display the content of the array
print(my_array)
