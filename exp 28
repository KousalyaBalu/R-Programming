# Create a factor from a random sample of LETTERS
set.seed(123)  # Setting seed for reproducibility
letters_sample <- sample(LETTERS, 100, replace = TRUE)
letters_factor <- factor(letters_sample)

# Extract five levels from the factor
selected_levels <- levels(letters_factor)[1:5]

# Print the original factor and the selected levels
cat("Original Factor:\n")
print(letters_factor)

cat("\nSelected Levels:\n")
print(selected_levels)
