# Sample vector
vector_data <- c(1, 2, 3, 4, 5)

# Sample matrix
matrix_data <- matrix(c(6, 7, 8, 9, 10, 11), nrow = 2, ncol = 3)

# Sample function
multiply_by_two <- function(x) {
  return(x * 2)
}

# Create a list with vectors, matrices, and functions
my_list <- list(
  vector_element = vector_data,
  matrix_element = matrix_data,
  function_element = multiply_by_two(2)
)

# Print the content of the list
print(my_list)
